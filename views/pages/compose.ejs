<body class="">

    <div class="container">
        <%- include('../partials/navbar.ejs',{user:false}) %>
        <div class="row">
            <div class="col-1">
                <button onclick="history.back(-1)" class="btn"><i class="fas fa-arrow-left"></i></button>
            </div>
            <div class="col">

                <form>
                    <div class="form-group row">
                        <label for="text" class="col-1 col-form-label">To:</label>
                        <div class="col-8">
                            <input id="text" name="text" placeholder="Enter username" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="text1" class="col-1 col-form-label">Subject:</label>
                        <div class="col-8">
                            <input id="text1" name="text1" placeholder="Describe your message" type="text"
                                class="form-control">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="offset-2 col-10">
                        </div>
                    </div>

                    <div class="m-3">
                        <div id="editor">
                            <p>Hello World!</p>
                            <p>Some initial <strong>bold</strong> text</p>
                            <p><br></p>
                        </div>
                    </div>
                    <button name="submit" type="submit" class="btn btn-success m-3">Send Message</button>
                </form>



            </div>
        </div>
        <!-- Include the Quill library -->


        <!-- Initialize Quill editor -->


        <div class="row">
            <div class="col">

            </div>
        </div>


